            <div class="EgerichteKategorien">
                <img src="<%= element.image %>" alt="">
                <div class="Epreis">
                     <h5><%= element.title %></h5>
                     <h4><%= element.price %></h4>
                </div>
                <div class="EBorderBottom">
                     <h1></h1>
                     <h6>(â‚¬Inf/kg)</h6>
                </div>
                <div class="ESofort">
